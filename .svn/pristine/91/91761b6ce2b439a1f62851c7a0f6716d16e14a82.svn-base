.login-form input[type="text"], .login-form input[type="password"] {
	font-size: 1em;
	color: #666;
	padding: 1rem 1em;
	border: 0;
	width: 90%;
	background: none;
	line-height: 3rem;
	-webkit-appearance: none;/*改变按钮和其他控件的外观，使其类似于原生控件。*/
}
.styled-input input::-webkit-input-placeholder {/* WebKit browsers */
	color: #666;
}
.styled-input input::-moz-placeholder {/* Mozilla Firefox 4 to 18 */
	color: #666;
}
.styled-input input::-moz-placeholder {/* Mozilla Firefox 19+ */
	color: #666;
}
.styled-input input::-ms-input-placeholder {/* Internet Explorer 10+ */
	color: #666;
}
.styled-input input:focus {
	outline: 0;
}
.styled-input {
	width: 100%;
	margin: 0 0;
	border-bottom: 1px solid #ccc;
	float: right;
	color: #666;
	height: 5rem;
